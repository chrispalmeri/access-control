[
    {
        "name": "doorctl",
        "host": "doorctl-dev",
        "protocol": "sftp",
        "uploadOnSave": true,
        "username": "doorctl",
        "ignore": [
            "__pycache__/"
        ],
        "context": "./code",
        "remotePath": "/home/doorctl/access-control/code",
        "watcher": {
            "files": "www/**/*.{js,css,ico}",
            "autoUpload": true,
            "autoDelete": true
        },
        "remoteExplorer": {
            "filesExclude": [
                "**/__pycache__"
            ]
        },
        "sshCustomParams": "'cd \"${remotePath}/../\"; exec $SHELL -l'"
    }
]
